{
  "document": {
    "indent": 2,
    "background_color": "234",
    "prefix": "\n",
    "suffix": "\n\n"
  },
  "link": {
    "color": "123",
    "underline": true,
    "prefix": "(",
    "suffix": ")"
  },
  "link_text": {
    "color": "123",
    "bold": true
  },
  "image": {
    "color": "123",
    "prefix": "[ðŸ“· ",
    "suffix": "]"
  },
  "image_text": {
    "color": "8"
  },
  "heading": {
    "prefix": "\n",
    "suffix": "\n",
    "margin": 4,
    "color": "15",
    "background_color": "57"
  },
  "h1": {
    "format": "=> {{(Join (Split .text \"\") \" \")}} <=",
    "margin": 2,
    "bold": true,
    "background_color": "69"
  },
  "h2": {
    "format": "# {{.text}}"
  },
  "table": {
    "margin": 4
  },
  "paragraph": {
    "margin": 4,
    "color": "15",
    "background_color": "235"
  },
  "code": {
    "color": "200"
  },
  "code_block": {
    "margin": 4,
    "color": "200",
    "theme": "solarized-dark"
  },
  "emph": {
    "italic": true
  },
  "strong": {
    "bold": true
  },
  "list": {
    "margin": 4,
    "color": "15",
    "background_color": "236"
  },
  "item": {
    "format": "â€¢ {{.text}}"
  },
  "enumeration": {
    "format": ". {{.text}}"
  },
  "hr": {
    "format": "\n---\n"
  }
}
