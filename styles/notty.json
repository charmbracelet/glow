{
  "document": {
    "indent": 4,
    "prefix": "\n\n",
    "suffix": "\n\n"
  },
  "h1": {
    "format": "# {{.text}}",
    "suffix": "\n\n"
  },
  "h2": {
    "prefix": "\n",
    "suffix": "\n",
    "format": "## {{.text}}"
  },
  "h3": {
    "format": "### {{.text}}",
    "suffix": "\n"
  },
  "h4": {
    "format": "#### {{.text}}",
    "suffix": "\n"
  },
  "h5": {
    "format": "##### {{.text}}",
    "suffix": "\n"
  },
  "h6": {
    "format": "###### {{.text}}",
    "suffix": "\n"
  },
  "text": {},
  "list": {},
  "item": {
    "prefix": "â€¢ "
  },
  "enumeration": {
    "prefix": ". "
  },
  "link_text": {},
  "link": {
    "format": "({{.text}})"
  },
  "image_text": {
    "format": "Image: {{.text}} ->"
  },
  "image": {},
  "code": {
    "format": "`{{.text}}`"
  },
  "code_block": {
    "margin": 4,
    "prefix": "\n",
    "suffix": "\n"
  },
  "table": {},
  "emph": {
    "prefix": "*",
    "suffix": "*"
  },
  "strong": {
    "prefix": "**",
    "suffix": "**"
  },
  "hr": {
    "format": "\n--------\n"
  },
  "block_quote": {},
  "del": {},
  "softbreak": {},
  "hardbreak": {},
  "html_block": {},
  "html_span": {}
}
