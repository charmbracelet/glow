    "prefix": "*",
    "suffix": "*"
  },
  "strong": {
    "prefix": "**",
    "suffix": "**"
  },
  "hr": {
    "format": "\n--------\n"
  },
  "block_quote": {
    "prefix": "> "
  },
  "html_block": {},
  "html_span": {}
